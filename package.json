{
  "name": "eslint-transforms",
  "version": "2.0.0",
  "description": "Codemods for upgrading eslint rules",
  "license": "MIT",
  "author": "Vitor Balocco",
  "repository": "eslint/eslint-transforms",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "bin": {
    "eslint-transforms": "./bin/eslint-transforms.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build:new-rule-format": "node ./bin/eslint-transforms.js -t ./lib/new-rule-format/new-rule-format.js",
    "lint": "eslint .",
    "release:generate:latest": "eslint-generate-release",
    "release:generate:alpha": "eslint-generate-prerelease alpha",
    "release:generate:beta": "eslint-generate-prerelease beta",
    "release:generate:rc": "eslint-generate-prerelease rc",
    "release:publish": "eslint-publish-release",
    "test": "mocha ./tests/lib/**/*.js"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^8.45.0",
    "eslint-config-eslint": "^8.0.0",
    "eslint-release": "^1.0.0",
    "globals": "^13.20.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "jscodeshift": "^0.3.20"
  },
  "keywords": [
    "javascript",
    "eslint",
    "jscodeshift"
  ]
}

{
  "name": "eslint-transforms",
  "version": "0.0.1",
  "description": "Codemods for upgrading eslint rules",
  "license": "MIT",
  "author": "Vitor Balocco",
  "repository": {
    "type": "git",
    "url": "https://github.com/eslint/eslint-transforms"
  },
  "engines": {
    "node": ">=0.10"
  },
  "bin": {

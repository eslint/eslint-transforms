    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.9.0",
    "eslint-config-eslint": "^3.0.0",
    "eslint-release": "^0.5.0",
    "jest-cli": "^12.0.2"
  },
  "dependencies": {
    "jscodeshift": "^0.3.20",
    "shelljs-nodecli": "^0.1.1"
  }
